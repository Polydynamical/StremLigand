	const recaptchaButtons = document.querySelectorAll("button[class='g-recaptcha']");
	for (const button of recaptchaButtons) {
		button.setAttribute('data-callback', 'recaptchaSubmit');
		button.setAttribute('data-sitekey', '6LdVLSEhAAAAAJD4cz8XqrWdGdrbI8Qnn-SvoXW8')

		button.addEventListener("mousedown", function(event) {
			event.preventDefault();
			event.target.setAttribute('clicked', "");
		});

		const internalInputs = button.querySelectorAll("input");

		for (const input of internalInputs) {
			input.addEventListener("mousedown", function(event) {
				event.preventDefault();
				event.target.parentElement.setAttribute('clicked', "");
			});
		}
	}

	function recaptchaSubmit(token) {
		const clickedButton = document.querySelector("button[class='g-recaptcha'][clicked='']");
		clickedButton.removeAttribute('clicked');
		var form = clickedButton.form;
		form.submit();
	}
